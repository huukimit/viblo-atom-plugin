{
  "name": "viblo-atom-plugin",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Plug-in to integrate with the service Viblo.asia - Free service for technical knowledge sharing",
  "keywords": [
    "atom",
    "markdown",
    "integration",
    "social sharing",
    "viblo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framgia/viblo-atom-plugin.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "bugs": {
    "url": "https://github.com/framgia/viblo-atom-plugin/issues"
  },
  "configSchema": {
    "apiToken": {
      "type": "string",
      "default": "",
      "description": "Paste your private VIBLO API token here...",
      "title": "To integrate with Viblo Sharing Platform you must to create new API token on Viblo Profile Page"
    }
  },
  "homepage": "https://github.com/framgia/viblo-atom-plugin#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "styles": [
    "styles/viblo-atom-plugin.less"
  ],
  "deserializers": {
    "VibloPluginSettingsView": "createSettingsView"
  },
  "author": "amelkikh",
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "fs-plus": "^2.9.3",
    "jquery": "^3.1.1",
    "markdown-it": "^8.2.2",
    "markdown-it-emoji": "^1.3.0",
    "marked": "^0.3.6",
    "semver": "^5.3.0",
    "space-pen": "^5.1.2",
    "underscore-plus": "^1.6.6"
  }
}
